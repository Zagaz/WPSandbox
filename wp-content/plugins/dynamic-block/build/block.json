{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "latest-posts/dynamic-block",
  "version": "0.1.0",
  "title": "ZAGAZ - Latest Posts",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "attributes": {
    "postsPerPage": {
      "type": "number",
      "default": 4
    },
    "order": {
      "type": "string",
      "default": "DESC"
    },
    "category": {
      "type": "string",
      "default": "all"
    },
    "author": {
      "type": "string",
      "default": "all"
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "dynamic-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}