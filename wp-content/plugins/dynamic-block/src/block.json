{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "latest-posts/dynamic-block",
	"version": "0.1.0",
	"title": "ZAGAZ - Latest Posts",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"postsPerPage": {
			"type": "number",
			"default": 4
		},
		"order": {
			"type": "array",
			"default": "DESC"
		},
		"category": {
			"type": "array"

		},
		"author":{
			"type": "array"
		},
		"allAuthors": {
			"type": "boolean",
			"default": true
		},
		"showFeaturedImage": {
			"type": "boolean",
			"default": false
		},
		"showExcerpt": {
			"type": "boolean",
			"default": false
		},
		"showAuthor": {
			"type": "boolean",
			"default": false
		},
		"showDate": {
			"type": "boolean",
			"default": false
		},
		"showCategories": {
			"type": "boolean",
			"default": false
		},
		"showTitle": {
			"type": "boolean",
			"default": true
		}



	},
	"example"
	: {},
	"supports": {
		"html": false
	},
	"textdomain": "dynamic-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
